<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/calendar_nav"
    app:startDestination="@id/startFragment">

    <fragment
        android:id="@+id/startFragment"
        android:name="jp.ac.it_college.std.s20020.calendar_u.StartFragment"
        android:label="fragment_start"
        tools:layout="@layout/fragment_start" >
        <action
            android:id="@+id/action_startFragment_to_todayScheduleFragment"
            app:destination="@id/todayScheduleFragment" />
    </fragment>
    <dialog
        android:id="@+id/todayScheduleFragment"
        android:name="jp.ac.it_college.std.s20020.calendar_u.TodayScheduleFragment"
        android:label="fragment_today_schedule"
        tools:layout="@layout/fragment_today_schedule" >
        <argument
            android:name="year"
            app:argType="integer"
            android:defaultValue="0" />
        <argument android:name="month"
            app:argType="integer"
            android:defaultValue="0" />
        <argument android:name="day"
            app:argType="integer"
            android:defaultValue="0" />
    </dialog>
</navigation>